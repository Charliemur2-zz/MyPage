{
  "name": "mypage",
  "version": "1.0.0",
  "description": "Mi primera pagina como artista.",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin images/* --out-dir dist/images",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin contacto.html -d dist --htmlmin -o dist/contacto.html && usemin demos.html -d dist --htmlmin -o dist/demos.html && usemin imagenes.html -d dist --htmlmin -o dist/imagenes.html && usemin videos.html -d dist --htmlmin -o dist/videos.html", 
    "build": "npm run clean && npm run scss npm run imagein && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Charliemur2/MyPage.git"
  },
  "author": "Carlos Andres Murcia Mu√±oz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Charliemur2/MyPage/issues"
  },
  "homepage": "https://github.com/Charliemur2/MyPage#readme",
  "devDependencies": {
    "copyfiles": "^2.3.0",
    "cssmin": "^0.4.3",
    "htmlmin": "0.0.7",
    "lite-server": "^2.5.4",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "fontello": "^0.5.0",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "sass": "^1.26.10"
  }
}
